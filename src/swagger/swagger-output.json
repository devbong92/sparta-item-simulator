{
  "openapi": "3.1.0",
  "info": {
    "version": "1.0.0",
    "title": "Item Simulator",
    "description": "개인과제"
  },
  "servers": [
    {
      "url": "http://localhost:3020",
      "description": ""
    }
  ],
  "tags": [
    {
      "name": "",
      "description": ""
    }
  ],
  "paths": {
    "/sign-up": {
      "post": {
        "tags": [
          "회원가입"
        ],
        "description": "회원가입 - 유저",
        "responses": {
          "201": {
            "description": "Created"
          },
          "409": {
            "description": "Conflict"
          }
        },
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/signUp"
              }
            }
          }
        }
      }
    },
    "/sign-in": {
      "post": {
        "tags": [
          "로그인"
        ],
        "description": "로그인 - 유저",
        "responses": {
          "200": {
            "description": "OK"
          },
          "401": {
            "description": "Unauthorized"
          }
        },
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/signIn"
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "signUp": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@email.com"
          },
          "password": {
            "type": "string",
            "example": "qwer1234"
          },
          "name": {
            "type": "string",
            "example": "김유저"
          }
        },
        "required": [
          "email",
          "password",
          "name"
        ]
      },
      "signIn": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@email.com"
          },
          "password": {
            "type": "string",
            "example": "qwer1234"
          }
        },
        "required": [
          "email",
          "password"
        ]
      }
    }
  }
}